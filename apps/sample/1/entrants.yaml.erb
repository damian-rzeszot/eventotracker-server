---
entrants:
  <% 50.times do |index| %>

  - id: <%= 1000 + index + 1 %>
    number: <%= index + 1 %>
    name: Team <%= Faker::Color.color_name.capitalize %>
    info: |-
      <%= Faker::University.name %>
      <%= Faker::Gender.binary_type %>
    extra: |-
      <%= Faker::Name.name %>
      <%= Faker::Name.name %>

  <% end %>
